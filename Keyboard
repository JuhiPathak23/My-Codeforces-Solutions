#include<bits/stdc++.h>
using namespace std;
int main(){
  string k = "qwertyuiopasdfghjkl;zxcvbnm,./";
  char o;
  cin>>o;
  string s;
  cin>>s;
  if(o == 'R'){
    for(int i = 0; i < s.length(); i++)
      s[i] = k[k.find(s[i]) - 1];
  }
  else{
    for(int i = 0; i < s.length(); i++)
      s[i] = k[k.find(s[i]) + 1];
  }
  cout<<s<<endl;
}


https://codeforces.com/problemset/problem/474/A
