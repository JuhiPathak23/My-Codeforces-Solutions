#include<bits/stdc++.h>
using namespace std;
int main(){
    int i,j,k;
    int n,m;
    cin>>n>>m;
    int a[n], b[m];
    for(i=0;i<n;i++)
        cin>>a[i];
    for(i=0;i<m;i++)
        cin>>b[i];
    int x=0, cnt=0;
    for(i=0;i<n;i++){
        if(x<m && a[i]<=b[x]){
            cnt++;
            x++;
        }
    }
    cout<<cnt;
    return 0;
}

https://codeforces.com/problemset/problem/1009/A
