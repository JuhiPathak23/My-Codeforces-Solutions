#include<bits/stdc++.h>
using namespace std;
int main(){
  int n,a,b,x=0,y=0;
  cin>>n;
  while(n--){
    cin>>a>>b;
    if(a>b){
      x++;
      continue;
    }
    if(b>a){
      y++;
      continue;
    }
  }
  if(x>y){
    cout<<"Mishka"<<endl;
    return 0;
  }
  if(y>x){
    cout<<"Chris"<<endl;
    return 0;
  }
  cout<<"Friendship is magic!^^"<<endl;
  return 0;
}


https://codeforces.com/problemset/problem/703/A
