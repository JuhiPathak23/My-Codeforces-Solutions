#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        long long int n;
        cin>>n;
        for(int i=0;i<n;i++){
            long long int x;
            cin>>x;
            cout<<(n+1)-x<<" ";
        }
        cout<<endl;
    }
}

https://codeforces.com/problemset/problem/1831/A
