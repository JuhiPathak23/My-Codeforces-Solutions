#include<bits/stdc++.h>
using namespace std;
int main(){
  int t;
  cin>>t;
  int n;
  while(t--){
    cin>>n;
    deque<int>v;
    int x;
    for(int i = 0; i < n; i++){
      cin>>x;
      v.push_back(x);
    }
    bool f=false;
    for(int i = 0; i < n; i++){
      f=!f;
      if(f){
        cout<<v.front()<<" ";
        v.pop_front();
      }
      else{
        cout<<v.back()<<" ";
        v.pop_back();
      }
    }
    cout<<endl;
  }
}


https://codeforces.com/problemset/problem/1462/A
