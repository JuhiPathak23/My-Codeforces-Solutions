#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        int hh,mm;
        cin>>hh>>mm;
        if(mm==0)
        cout<<(24-hh)*60<<endl;
        else
        cout<<(24-hh-1)*60 + (60-mm)<<endl;
    }
    return 0;
}



https://codeforces.com/problemset/problem/1283/A
