#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main(){
  int t;
  cin>>t;
  ll n;
  while(t--){
    cin>>n;
    ll a, b, c, max = -1;
    cin>>a;
    for(ll i = 1; i < n; i++){
      cin>>b;
      c = a * b;
      max = (c > max) ? c : max;
      a = b;
    }
    cout<<max<<endl;
  }

}


https://codeforces.com/problemset/problem/1554/A
