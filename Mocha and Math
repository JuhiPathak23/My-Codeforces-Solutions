#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main(){
  ll t;
  cin>>t;
  while(t--){
    ll n;
    cin>>n;
    ll arr[n];
    for(ll i=0;i<n;i++)
      cin>>arr[i];
    ll res = arr[0];
    for(ll i=1;i<n;i++)
      res &= arr[i];
    cout<<res<<endl;
  }
}


https://codeforces.com/problemset/problem/1559/A
