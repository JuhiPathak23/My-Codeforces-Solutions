#include<bits/stdc++.h>
using namespace std;
int main(){
    long long i,j,x,m,n;
    while(cin>>n){
        long long arr[200000];
        for(i=0;i<n;i++)
            cin>>arr[i];
        long long res=0;
        for(i=0;i<n;i++){
            if(i>0)
               res+=abs(arr[i]-arr[i-1]);
            else
               res+=abs(arr[i]);
        }
        cout<<res<<endl;
    }
    return 0;
}


https://codeforces.com/problemset/problem/596/B
