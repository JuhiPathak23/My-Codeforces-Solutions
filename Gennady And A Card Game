#include<bits/stdc++.h>
using namespace std;
int main(){
  string a;
  cin>>a;
  string arr[5];
  for(int i = 0; i < 5; i++)
    cin>>arr[i];
  for(int i = 0; i < 5; i++){
    if(a[0] == arr[i][0]){
      cout<<"YES"<<endl;
      return 0;
    }
  }
  for(int i = 0; i < 5; i++){
    if(a[1] == arr[i][1]){
      cout<<"YES"<<endl;
      return 0;
    }
  }
  cout<<"NO"<<endl;
}


https://codeforces.com/problemset/problem/1097/A
