#include<bits/stdc++.h>
using namespace std;
int main(){
  int n, p, q;
  cin>>n;
  cin>>p;
  int x;
  set<int,less<int>>s;
  for(int i=0;i<p;i++){
    cin>>x;
    s.insert(x);
  }
  cin>>q;
  for(int i=0;i<q;i++){
    cin>>x;
    s.insert(x);
  }
  int c=s.size()==n?1:0;
  if(c==1)
    cout<<"I become the guy.";
  else
    cout<<"Oh, my keyboard!";
  return 0;
}

https://codeforces.com/problemset/problem/469/A
