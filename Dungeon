#include<bits/stdc++.h>
using namespace std;
int main(){
  int t;
  cin>>t;
  while(t--){
    int a,b,c;
    cin>>a>>b>>c;
    int sum = a + b + c;
    if (sum % 9 == 0 && min({a, b, c}) >= sum / 9) 
      cout << "YES\n";
    else
      cout << "NO\n";
  }
  return 0;
}


https://codeforces.com/problemset/problem/1463/A
