#include<bits/stdc++.h>
using namespace std;
int main(){
  int t;
  cin>>t;
  int n, x, m;
  while(t--){
    cin>>n>>x>>m;
    while(n > 0 && m > 0){
      if(n > 20 && x > 0){
        n /= 2;
        n += 10;
        x--;
      }
      else{
        n -= 10;
        m--;
      }
    }
    (n <= 0) ? cout<<"YES"<<endl : cout<<"NO"<<endl;
  }
}


https://codeforces.com/problemset/problem/1337/B
