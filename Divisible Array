#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        if(n%2){
            for(int i=1;i<=n;i++)
                cout<<i<<" ";
        }
        else{
            int x=2;
            for(int i=0;i<n;i++){
                cout<<x<<" ";
                x+=2;
            }
        }
        cout<<endl;
    }
}

https://codeforces.com/problemset/problem/1828/A
