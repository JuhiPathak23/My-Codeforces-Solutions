#include<bits/stdc++.h>
using namespace std;
int main(){
  int t;
  cin>>t;
  while(t--){
    int a[3];
    cin>>a[0]>>a[1]>>a[2];
    sort(a, a + 3, greater<int>());
    if(a[0] == a[1]){
      cout<<"YES"<<endl;
      cout<<a[0]<<" "<<a[2]<<" "<<a[2]<<endl;
      continue;
    }
    cout<<"NO"<<endl;
  }
}


https://codeforces.com/problemset/problem/1385/A
